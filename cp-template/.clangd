CompileFlags:
	Add:
		- -std=c++20
		- -Wall
		- -Wextra
		- -Wshadow
		- -Wnon-virtual-dtor
		- -Wold-style-cast
		- -Wcast-align
		- -Wunused
		- -Woverloaded-virtual
		- -Wpedantic
		- -Wconversion
		- -Wsign-conversion
		- -Wmisleading-indentation
		- -Wduplicated-cond
		- -Wduplicated-branches
		- -Wlogical-op
		- -Wnull-dereference
		- -Wuseless-cast
		- -Wformat=2
		- -Wformat-overflow
		- -Wformat-truncation
		- -Wdouble-promotion
		- -Wundef
		- -DLOCAL
		- -Wno-unknown-pragmas